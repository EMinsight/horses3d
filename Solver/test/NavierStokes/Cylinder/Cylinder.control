Flow equations        = "NS"
mesh file name        = "../../TestMeshes/CylinderNSpol3.mesh"
plot file name        = "RESULTS/Cylinder.tec"
restart file name     = "RESULTS/Cylinder.rst"
save file name        = "RESULTS/Cylinder_saved.rst"
restart               = .false.
Polynomial order      = 3
Number of time steps  = 100
Output Interval       = 10
Number of plot points = 5
Convergence tolerance = 1.d-10
cfl                   = 0.3
mach number           = 0.3
Reynolds number       = 200.0
AOA theta             = 0.0
AOA phi               = 90.0
